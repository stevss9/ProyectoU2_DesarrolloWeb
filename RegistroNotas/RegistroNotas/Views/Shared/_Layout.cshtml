<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Registro de Notas</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/cdn.datatables.net_1.13.5_css_dataTables.bootstrap5.min.css")
    @RenderSection("style", required: false)
<style>
    body{
        background-color:wheat;
        font-family:'Times New Roman';
        color:black;
    }
    thead{
        background-color:black;
        color:white;
        font-size:18px;
    }
    tbody {
        background-color: white;
        color: black;
        font-size: 18px;
    }

</style>
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @Scripts.Render("~/Scripts/cdn.datatables.net_1.13.5_js_jquery.dataTables.min.js")
    @Scripts.Render("~/Scripts/cdn.datatables.net_1.13.5_js_dataTables.bootstrap5.min.js")
    @RenderSection("scripts", required: false)


</body>
</html>
